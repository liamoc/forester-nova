<?xml version="1.0" encoding="UTF-8"?>
<syntax name="forester_inline" subsyntax="true">
	<meta>
		<name>Forester Inline</name>
	</meta>
	
	<brackets>
		<pair open="{" close="}" />
		<pair open="[" close="]" />
		<pair open="(" close=")" />
	</brackets>
	<comments>
		<single>
			<expression>% </expression>
		</single>
	</comments>
	<tree-sitter language="markdown_inline">
		<highlights path="highlights-inline.scm" />
	</tree-sitter>
</syntax>
